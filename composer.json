{
  "name": "partimate/throttlemate",
  "description": "Adaptive Throttling for Laravel to handle connection bottleneck with Graceful Degradation",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Deden Farhan",
      "email": "farhanindo21@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PartiMate\\ThrottleMate\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "PartiMate\\ThrottleMate\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
